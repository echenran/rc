# General shortcuts
# ls all
alias ll='ls -la'
# ls + sort by file size + display K, M, G, etc.
alias lss='ls -lahS'
# ls + sort by last added with most recent at bottom
alias la='ls -laUr'
# ls + sort by last modified with most recent at bottom
alias lm='ls -latr'
# ls + sort by last accessed with most recent at bottom
alias lac='ls -laur'

